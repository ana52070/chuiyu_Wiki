{
  "buildCommand": "npm run docs:build",
  "outputDirectory": ".vitepress/dist",
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}